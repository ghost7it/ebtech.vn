<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    @Html.Partial("_Head")
    @RenderSection("styles", required: false)
</head>
<body>
    <div id="wrapper">
        @Html.Partial("_Header")
        @RenderBody()
        @Html.Partial("_Footer")
    </div>
    @Html.Partial("_Scripts")
    @RenderSection("scripts", required: false)
</body>
</html>
